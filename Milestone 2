def perfect_match(rna):
    import math
    countU = 0
    countA = 0
    countC = 0
    countG = 0
    for i in range(len(rna)):
        if rna[i] == 'U':
            countU += 1
        if rna[i] == 'A':
            countA += 1
        if rna[i] == 'C':
            countC += 1
        if rna[i] == 'G':
            countG += 1
    perfect_matchings = math.factorial(countU) * math.factorial(countC)
    if countU != countA:
        perfect_matchings = 0
    if countC != countG:
        perfect_matchings = 0
    return perfect_matchings

def random_genome(dna, gc_content):
    import math
    probabilities = []
    for probability in gc_content:
        result = 0
        for letter in dna:
            if letter == 'G':
                result += math.log10(probability/2)
            if letter == 'C':
                result += math.log10(probability/2)
            if letter == 'A':
                result += math.log10((1-probability)/2)
            if letter == 'T':
                result += math.log10((1-probability)/2)
        probabilities.append(result)
    return probabilities

